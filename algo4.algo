ALGORITHM Checkpoint Sorting and searching algorithms
VAR
    i,j,n : INTEGER
    tab : array_name : ARRAY_OF INTEGER[n]; //input 
    tab : array_name : ARRAY_OF INTEGER[n]; //output 
BEGIN
    FOR i FROM 0 TO n   DO
            v = tab[i] ;
            j=i ;
            WHILE (tab[j-1]>v) DO
                tab[j]=tab[j-1] ;
                j=j-1
            END_WHILE
            tab[j]=v 
    END_FOR  
END
